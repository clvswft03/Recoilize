{
  "name": "@todaypp/recoilize",
  "version": "3.0.2",
  "description": "A Chrome extension that helps debug Recoil applications by memorizing the state of components with every render.",
  "author": "Bren Yamaguchi, Saejin Kang, Jonathan Escamilla, Sean Smith, Justin Choo, Anthony Lin, Spenser Schwartz, Steven Nguyen, Henry Taing, Seungho Baek, Taven Shumaker, Aaron Yang, Jesus Vargas, Davide Molino, Janis Hernandez, Jaime Baik, Anthony Magallanes, Edward Shei, Leonard Lew, Joey Ma, Harvey Nguyen, Victor Wang, Sumin Son",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/todaypp/recoilize.git"
  },
  "bugs": {
    "url": "https://github.com/todaypp/recoilize/issues"
  },
  "homepage": "https://github.com/todaypp/recoilize#readme",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "recoil",
    "recoilize",
    "fix",
    "state",
    "state-manager",
    "chrome",
    "devtools",
    "visualize",
    "visualizer",
    "debug",
    "debugger"
