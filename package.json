  "main": "index.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  },
  "directories": {
    "doc": "docs"
  },
  "jest": {
    "setupFiles": [
      "jest-webextension-mock"
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "clean": "rimraf node_modules src/extension/{build/bundles,*{.crx,.pem}}",
    "dev": "webpack --mode development --watch",
    "install:clean": "rimraf yarn.lock && yarn clean && yarn install",
    "lint": "eslint '**/*.tsx' '**/*.ts' '**/*.js' --ignore-path .gitignore",
    "posttest": "publish",
    "test": "jest --verbose"
